<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <!--
    Data extraction rules for devices running Android 12 (API 31) and higher.
    See https://developer.android.com/about/versions/12/backup-restore for details.
    -->
    <cloud-backup>
        <include domain="sharedpref" path="."/>
        <exclude domain="sharedpref" path="device_specific_prefs.xml"/>
    </cloud-backup>
    <device-transfer>
        <include domain="sharedpref" path="."/>
        <exclude domain="sharedpref" path="device_specific_prefs.xml"/>
    </device-transfer>
</data-extraction-rules>
